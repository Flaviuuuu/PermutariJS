var s = [1, 2, 3, 4];
var k = 0;
var valid = false;

function permute(n){
  var k = 0;
  valid = false;
  s = [];
  for (i = 0; i < n; i++) {
    s[i] = 0;
  }

  while (k >= 0) {
    s[k]++;
    valid = false;

    while (!valid && s[k] <= n) {
      if (ver(s, k + 1)) {
        valid = true;
      } else {
        s[k]++;
      }
    }
